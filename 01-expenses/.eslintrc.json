{
  "env": {
    "browser": true,
    "es2021": true
  },
  "settings": {
    "react": { "pragma": "React", "version": "detect" }
  },
  "extends": ["plugin:react/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "quotes": "off",
    "@typescript-eslint/quotes": ["warn", "single"],
    "react/jsx-key": "off",
    "react/prop-types": "off"
  }
}
